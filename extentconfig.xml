<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>standard</theme>
    
        <!-- enables timeline -->
        <!-- defaults to true -->
        <enableTimeline>true</enableTimeline>
	
        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>
        
        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>
        
        <!-- title of the document -->
        <documentTitle>Framework</documentTitle>
        
        <!-- report name - displayed at top-nav -->
        	<reportName> 
		 <![CDATA[ 
                <img src='src/test/resources/logo/teqsar_logo_80.jpg' /> 
            ]]> 
		</reportName> 

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
        </scripts>
        
        <!-- custom styles -->
        <styles>
            <![CDATA[
                
            ]]>
        </styles>
    </configuration>
</extentreports>
